# GLM Agent Model Configuration (optional - falls back to default if not set)
GLM_MODEL_BASE_URL=
GLM_MODEL_NAME=
GLM_MODEL_API_KEY=

# GELab Agent Model Configuration (optional - falls back to default if not set)
GELAB_MODEL_BASE_URL=
GELAB_MODEL_NAME=gelab-zero-4b-preview
GELAB_MODEL_API_KEY=

# Server Configuration
HOST=0.0.0.0
PORT=8080
# Set to false in production to disable /docs, /redoc, /openapi.json
DEBUG=false

# CORS Origins (comma-separated)
CORS_ORIGINS=http://localhost:5173,http://localhost:3000
